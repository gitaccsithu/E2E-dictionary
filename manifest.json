{
    "manifest_version": 2,
    "name": "EEDictionary",
    "version": "0.0.1",
    "content_scripts": 
    [
        {
            "matches": 
            [
                "<all_urls>"
            ],
            "js": 
            [
                "content.js"
            ],
            "css":
            [
                "style.css"
            ]
        }
    ],
    "icons" : {"128" : "popup/icon.png"},
    "permissions": [ "management" ],
    "background":
    {
        "scripts": 
        [
            "background.js"
        ]
    },
    "browser_action": 
    {
        "default_icon": "popup/icon.png",
        "default_popup": "popup/popup.html"
    }
}